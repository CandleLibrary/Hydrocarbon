		│ ... assignment_expression  					↦ cstr ^spread_expr
		│ argument_list , assignment_expression 		↦ cstr return (sym[0].push(sym[2]), sym[0])
		│ argument_list , ... assignment_expression 	↦ cstr return (sym[0].push(new env.functions.spread_expr(env, sym.slice(2,4))), env[0])


